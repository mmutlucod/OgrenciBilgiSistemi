@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers


<h3><b>@ViewBag.SinavBilgi.DersAdi</b> Dersi <b>@ViewBag.SinavBilgi.SinavTuru</b> Sınavı </h3>
<br />
<h5>Notu Girilen Öğrenciler</h5>
<br />
<table class="table">
    <thead>
        <tr>
            <th>Öğrenci Adı</th>
            <th>Sınav Notu</th>
            <th>İşlemler</th>

        </tr>
    </thead>
    <tbody id="tableBody">
        @foreach (var item in ViewBag.resultData)
        {
            <tr>
                <td>@item.OgrenciAdi @item.OgrenciSoyadi</td>

                <td>@item.SinavNotu</td>

                <td>
                    <a asp-controller="OgretimElemani" asp-action="NotDuzenle" asp-route-id="@item.DegerlendirmeID" asp-route-ogrenciID="@item.OgrenciID" asp-route-sinavID="@item.SinavID" class="btn btn-primary">Not Düzenle</a>
                </td>

                
            </tr>
        }
    </tbody>
    </table>
<br />
<br />
<h5>Notu Girilmeyen Öğrenciler</h5>
<br />
<table class="table">
    <thead>
        <tr>
            <th>Öğrenci Adı</th>
            <th>Sınav Notu</th>
            <th>İşlemler</th>

        </tr>
    </thead>
    <tbody id="tableBody">
        @foreach (var item in ViewBag.resultData1)
        {
            <tr>
                <td>@item.OgrenciAdi @item.OgrenciSoyadi</td>
                <td>@item.SinavNotu</td>


                <td>
                    <a asp-controller="OgretimElemani" asp-action="NotEkle" asp-route-ogrenciID="@item.OgrenciID" asp-route-sinavID="@item.SinavID" class="btn btn-primary">Not Ekle</a>
                </td>

                


            </tr>
        }
    </tbody>
