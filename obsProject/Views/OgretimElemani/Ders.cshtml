@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

    <h4>Verilen Dersler</h4>
    <div class="alert alert-success" role="alert">
        Ders Kayıt Onay işlemleri seçilen dersin Öğrenci Listesi ekranında yapılmaktadır.
    </div>
        <div id="veriGoster"></div>
        <div class="row mt-3">
            <div class="col-md-12">


                <table class="table">
                    <thead>
                        <tr>
                            <th>Dersin Kodu</th>
                            <th>Dersin Adı</th>
                            <th>Dersin Türü</th>
                            <th>Teorik (T)</th>
                            <th>Uygulama (U)</th>
                            <th>Kredi (K)</th>
                            <th>ECTS</th>
                            <th>İşlemler</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in ViewBag.ResultData)
                        {
                            <tr>
                                <td>@item.DersKodu</td>
                                <td>@item.DersAdi</td>
                                <td>@item.DersTuruAdi</td>
                                <td>@item.Teorik</td>
                                <td>@item.Uygulama</td>
                                <td>@item.Kredi</td>
                                <td>@item.ECTS</td>
                                <td>
                                    <a asp-controller="OgretimElemani" asp-action="DersOnay" asp-route-id="@item.DersAcmaID" class="btn btn-primary">Öğrenci Listesi</a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>



